{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@types": [
        "./app/types/index.ts"
      ],
      "@interrogation-content": [
        "./app/components/second/interrogation-content/index.ts"
      ],
      "@component/*": [
        "./app/components/second/*"
      ],
      "@accets/*": [
        "./app/Accets/*"
      ],
      "@api": [
        "./app/service/api/index.ts"
      ],
      "@utils": [
        "./app/service/utils/index.ts"
      ],
      "@admin-components": [
        "./app/admin/components/index.ts"
      ],
      "@hooks": [
        "./app/hooks/index.ts"
      ],
      "@pages/*": [
        "./pages/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/app/**/*.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
